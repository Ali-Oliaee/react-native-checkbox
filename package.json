{
  "name": "@react-native-community/checkbox",
  "version": "0.2.2",
  "description": "React Native Checkbox native modules for Android",
  "keywords": [
    "checkbox",
    "react native",
    "react-native"
  ],
  "homepage": "https://github.com/react-native-community/react-native-checkbox#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/react-native-community/react-native-checkbox.git"
  },
  "license": "MIT",
  "author": "Nicholas Lee <nicholaslee119@gmail.com>",
  "main": "dist/CheckBox",
  "types": "dist/CheckBox.android.d.ts",
  "scripts": {
    "prepare": "yarn build",
    "build": "tsc",
    "ci:test:type-check": "yarn test:type-check",
    "ci:test:js": "yarn test:js",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "yarn test:eslint && yarn test:type-check && yarn test:jest",
    "test:eslint": "eslint 'js/**/*.ts' 'js/**/*.tsx' 'example/**/*.js'",
    "test:type-check": "tsc --noEmit",
    "test:js": "jest"
  },
  "prettier": {
    "bracketSpacing": false,
    "jsxBracketSameLine": true,
    "parser": "flow",
    "singleQuote": true,
    "trailingComma": "all"
  },
  "jest": {
    "preset": "react-native"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@react-native-community/eslint-config": "^0.0.5",
    "@types/jest": "^24.0.25",
    "@types/react-native": "^0.60.27",
    "@types/react-test-renderer": "^16.9.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-module-resolver": "^3.1.3",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.1.2",
    "flow-bin": "^0.86.0",
    "jest": "^24.7.1",
    "metro-react-native-babel-preset": "0.51.1",
    "prettier": "1.19.1",
    "react": "16.6.3",
    "react-native": "0.58.4",
    "react-test-renderer": "^16.8.6",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.4"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "publishConfig": {
    "access": "public"
  }
}
